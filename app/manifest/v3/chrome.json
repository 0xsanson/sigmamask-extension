{
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'none'; frame-ancestors 'self';"
  },
  "externally_connectable": {
    "matches": ["https://metamask.io/*"],
    "ids": ["*"]
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "minimum_chrome_version": "88"
}
