    "@ethersproject/networks": "^5.7.0",
    "@ethersproject/properties": "^5.7.0",
    "@ethersproject/providers": "^5.7.0",
    "async-mutex": "^0.4.0",
    "body-parser": "^1.20.0",
    "commander": "^9.4.0",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "ethers": "^5.7.0",
    "express": "^4.18.1",
    "ow": "^0.28.1"
  },
